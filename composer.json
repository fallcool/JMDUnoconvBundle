{
  "name": "jmd/unoconv-bundle",
  "description": "This bundle allows you to convert files through unoconv (LibreOffice)",
  "keywords": ["symfony","file","convert"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksandr Kvintilyanov",
      "email": "bednyj.mops@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "symfony/symfony": "~2.7|~3.0",
    "php-unoconv/php-unoconv": "~0.3"
  },
  "autoload": {
    "psr-4": {
      "JMD\\UnoconvBundle\\": "."
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev"
}